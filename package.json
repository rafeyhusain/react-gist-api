{
  "name": "vite-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"npm run client:dev\" \"npm run storybook\"",
    "client:dev": "vite",
    "client:build": "vite build",
    "storybook:build": "build-storybook -o dist/storybook",
    "build": "npm run npm run client:build && npm run storybook:build",
    "test": "jest",
    "testw": "jest --watch",
    "serve": "vite preview",
    "prepare": "husky install",
    "lint": "tsc && prettier --check . && eslint . && stylelint \"**/*.css\"",
    "storybook": "start-storybook -p 6006",
    "start": "node dist/server.js"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@storybook/addon-actions": "^6.4.0-beta.19",
    "@storybook/addon-essentials": "^6.4.0-beta.19",
    "@storybook/addon-links": "^6.4.0-beta.19",
    "@storybook/react": "^6.4.0-beta.19",
    "@testing-library/jest-dom": "^5.16.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.4",
    "@types/react": "^17.0.32",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.3.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.3",
    "lint-staged": "^11.2.4",
    "postcss": "^8.4.4",
    "prettier": "2.4.1",
    "storybook-builder-vite": "^0.1.2",
    "stylelint": "^14.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^23.0.0",
    "tailwindcss": "^2.2.19",
    "ts-jest": "^27.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2",
    "vite": "^2.6.10"
  },
  "dependencies": {
    "@testing-library/react": "^12.1.2",
    "D": "^1.0.0",
    "dotenv": "^10.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0"
  }
}
